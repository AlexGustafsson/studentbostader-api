{
  "name": "studentbostader-api",
  "version": "0.0.2",
  "description": "An API to retrieve student accommondations from studentbostader.se",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava -v test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alexgustafsson/itslearning.git"
  },
  "author": "Alex Gustafsson",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.0",
    "parse-json": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "xo": "^0.18.1"
  },
  "xo": {
    "space": 2,
    "rules": {
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-use-before-define": [
        "error",
        {
          "classes": false
        }
      ],
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "error"
        }
      ],
      "dot-notation": [
        "off"
      ],
      "quote-props": [
        "off"
      ]
    }
  }
}
